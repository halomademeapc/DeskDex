<div class="col-md-12 mt-2">
    <div class="z-depth-1-half">
        <img alt="Floor Map" class="imageSelect" id="imageSelect" role="toolbar">
    </div>
    <div class="btn-toolbar float-md-right my-2" role="group">
        <div class="btn-group mr-2" aria-label="Drag Mode">
            <button type="button" class="btn btn-primary btn-sm" id="imageModePan"><i class="fa fa-arrows-alt" aria-hidden="true"></i></button>
            <button type="button" class="btn btn-outline-primary btn-sm waves-effect" id="imageModeReset">Drag</button>
            <button type="button" class="btn btn-primary btn-sm active" id="imageModeCrop"><i class="fa fa-crop" aria-hidden="true"></i></button>
        </div>
        <div class="btn-group mr-2" aria-label="Zoom Controls">
            <button type="button" class="btn btn-primary btn-sm" id="imageZoomout"><i class="fa fa-search-minus" aria-hidden="true"></i></button>
            <button type="button" class="btn btn-outline-primary btn-sm waves-effect" id="imageZoomReset">Zoom</button>
            <button type="button" class="btn btn-primary btn-sm" id="imageZoomin"><i class="fa fa-search-plus" aria-hidden="true"></i></button>
        </div>
        <div class="btn-group mr-2 d-none">
            <button type="button" class="btn btn-primary btn-sm" id="imageRotateCCW"><i class="fa fa-undo" aria-hidden="true"></i></button>
            <button type="button" class="btn btn-outline-primary btn-sm waves-effect" id="imageRotateReset">Rotate</button>
            <button type="button" class="btn btn-primary btn-sm" id="imageRotateCW"><i class="fa fa-redo" aria-hidden="true"></i></button>
        </div>
    </div>
    <div class="clearfix"></div>
</div>
